<snippet>
  <tabTrigger>h_user_delete</tabTrigger>
  <content><![CDATA[/**
 * Implements hook_user_delete().
 */
function ${TM_FILEPATH/.+(?:\/|\\)(?:themes|modules)(?:\/|\\)([a-zA-Z0-9_]+)(?:\/|\\).*/$1/}_user_delete(\$account) {
  db_delete('${1}')
    ->condition('uid', \$account->uid)
    ->execute();
}
]]></content>
</snippet>