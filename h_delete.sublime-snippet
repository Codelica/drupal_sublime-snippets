<snippet>
  <tabTrigger>h_delete</tabTrigger>
  <content><![CDATA[/**
 * Implements hook_delete().
 */
function ${TM_FILEPATH/.+(?:\/|\\)(?:themes|modules)(?:\/|\\)([a-zA-Z0-9_]+)(?:\/|\\).*/$1/}_delete(\$node) {
  db_delete('${1}')
    ->condition('nid', \$nid->nid)
    ->execute();
}
]]></content>
</snippet>